{
  "name": "where-to-go",
  "private": true,
  "scripts": {
    "start": "bnr start",
    "dev": "run-p dev:*",
    "dev:server": "bnr dev:server",
    "dev:client": "react-scripts start",
    "build": "npm-run-all build:*",
    "build:client": "react-scripts build",
    "build:server": "tsc -p server",
    "lint": "tslint -c tslint.json '**/*.ts' '**/*.tsx' --fix",
    "deploy": "now",
    "precommit": "npm-run-all lint"
  },
  "betterScripts": {
    "start": {
      "command": "node build/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "build/server"
      }
    },
    "dev:server": {
      "command": "tsc-watch -p server --preserveWatchOutput --pretty --onSuccess 'npm run start'",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "build/server"
      }
    }
  },
  "dependencies": {
    "@koa/cors": "2.2.2",
    "@material-ui/core": "3.0.1",
    "@material-ui/icons": "3.0.1",
    "ajv": "6.5.3",
    "got": "9.1.0",
    "koa": "2.5.2",
    "koa-favicon": "2.0.1",
    "koa-logger": "3.2.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.10",
    "query-string": "6.1.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-swipeable-views": "0.12.17",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "@types/got": "8.3.3",
    "@types/koa": "2.0.46",
    "@types/koa-favicon": "2.0.19",
    "@types/koa-logger": "3.1.0",
    "@types/koa-mount": "3.0.1",
    "@types/koa-router": "7.0.31",
    "@types/koa-static": "4.0.0",
    "@types/koa__cors": "2.2.3",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.2",
    "@types/query-string": "6.1.0",
